{
  "name": "@isplasher/typed-less-modules",
  "version": "0.2.4",
  "description": "TypeScript type definition generator for LESS CSS Modules",
  "main": "index.js",
  "license": "Apache",
  "repository": {
    "type": "git",
    "url": "https://github.com/isplasher-forks/typed-less-modules"
  },
  "homepage": "https://github.com/isplasher-forks/typed-less-modules#readme",
  "keywords": [
    "less",
    "less modules",
    "cli",
    "typescript",
    "type generator",
    "less modules"
  ],
  "scripts": {
    "test": "jest",
    "tlm": "ts-node ./lib/cli.ts",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && tsc && chmod +x dist/lib/cli.js",
    "prepare": "yarn build",
    "check-types": "tsc --noEmit",
    "format": "prettier --write '**/*.{js,json,css,md,less,tsx,ts}'",
    "check-formatting": "prettier --check '**/*.{js,json,css,md,less,tsx,ts}'",
    "codecov": "codecov",
    "commit": "commit",
    "ci": "yarn check-types && yarn check-formatting && yarn test"
  },
  "files": [
    "dist/lib"
  ],
  "bin": {
    "tlm": "./dist/lib/cli.js"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/prompt-cli": "^8.2.0",
    "@commitlint/travis-cli": "^8.2.0",
    "@types/camelcase": "^4.1.0",
    "@types/css-modules-loader-core": "^1.1.0",
    "@types/glob": "^7.1.1",
    "@types/jest": "^29.5.3",
    "@types/less": "^3.0.1",
    "@types/param-case": "^1.1.2",
    "@types/prettier": "^2.7.3",
    "@types/reserved-words": "^0.1.0",
    "@types/yargs": "^17.0.8",
    "babel-jest": "^29.6.2",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "codecov": "^3.7.0",
    "husky": "^1.3.1",
    "jest": "^29.6.2",
    "lint-staged": "^8.1.3",
    "prettier": "^2.8.8",
    "semantic-release": "^15.13.31",
    "ts-jest": "^23.10.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "less": ">3.0.0"
  },
  "dependencies": {
    "bundle-require": "^3.0.4",
    "camelcase": "^5.0.0",
    "chalk": "4.1.2",
    "change-case": "^4.1.2",
    "chokidar": "^3.5.3",
    "css-modules-loader-core": "^1.1.0",
    "esbuild": "^0.14.21",
    "glob": "^7.1.6",
    "joycon": "^3.1.1",
    "param-case": "^3.0.2",
    "path": "^0.12.7",
    "reserved-words": "^0.1.2",
    "slash": "^3.0.0",
    "yargs": "^17.3.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,less,tsx,ts}": [
      "prettier --write",
      "git add"
    ]
  }
}
